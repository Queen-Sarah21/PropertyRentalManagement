@model PropertyRentalManagement.Models.Manager

<h2>Manager's Details</h2>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th> ID </th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.ManagerId</td>
            <td>@Model.FirstName</td>
            <td>@Model.LastName</td>
            <td>@Model.Email</td>
            <td>@Model.Phone</td>
        </tr>
    </tbody>
</table>
